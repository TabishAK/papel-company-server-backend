{
  "name": "papel",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "link:commit": "commitlint -E HUSKY_GIT_PARAMS",
    "lint-staged": "lint-staged --concurrent false",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate -d src/config/company_database.config.ts",
    "migration:run": "yarn typeorm migration:run -d src/config/company_database.config.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/config/company_database.config.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-s3": "^3.723.0",
    "@aws-sdk/client-ses": "^3.758.0",
    "@aws-sdk/lib-storage": "^3.723.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^11.1.2",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.2",
    "@sendgrid/mail": "^8.1.4",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/pdfkit": "^0.13.9",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "form-data": "^4.0.3",
    "google-auth-library": "^9.15.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.0.2",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "mysql2": "^3.14.4",
    "openai": "^4.89.0",
    "otpauth": "^9.3.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.17.0",
    "redis": "^5.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "typeorm": "^0.3.26",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/lint": "^17.6.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.@(ts)": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
